{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
			"import/extensions":[1,"always" ],
			"object-curly-newline": [1, {
        "ObjectExpression": "always",
        "ObjectPattern": { "multiline": true },
        "ImportDeclaration":  { "multiline": false, "minProperties": 4 },
        "ExportDeclaration": { "multiline": false, "minProperties": 6 }
    }]
    }
}